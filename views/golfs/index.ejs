<!-- views/applications/index.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Posts</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %> 
    <h1>My Posts</h1>
    <a href="/users/<%= user._id%>/golfs/new">Add a Game</a>
    <ul>
      <% golfs.forEach((golf)=>{ %>
      <li>
        <a href="/users/<%= user._id %>/golfs/<%= golf._id %>">
        <%= golf.course%>
        </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
